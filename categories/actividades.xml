    &lt;div class="carousel-inner"&gt;
                &lt;div class="item active"&gt;&lt;img src="http://biblioln.es/2016/05/el_ultimo_holocausto_europeo.png" alt="" style="margin: 0 auto 0 auto;"&gt;&lt;/div&gt;
    &lt;/div&gt;
    &lt;a class="left carousel-control" href="http://biblioln.es/posts/2016/05/el-ultimo-holocausto-europeo.html#slides_81abc55860104e5dbe0b7ee20d02d9b9" data-slide="prev"&gt;&lt;span class="icon-prev"&gt;&lt;/span&gt;&lt;/a&gt;
